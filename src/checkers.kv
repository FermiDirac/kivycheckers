<WhitePiece>:
    canvas:
        Color:
            rgb: 1, 1, 1
        Ellipse:
            pos: self.pos
            size: self.size

<RedPiece>:
    canvas:
        Color:
            rgb: 1, 0, 0
        Ellipse:
            pos: self.pos
            size: self.size

<CheckersGame>:
    rows: 8
    cols: 8

