#:import r random.random

<CheckersGame>:
    cols: 8
    rows: 8


<WhiteSquare>:
    canvas:
        Color:
            rgb: 1, 1, 1
        Rectangle:
            pos: self.pos 
            size: self.size

<BlackSquare>:
    canvas:
        Color:
            rgb: 0, 0, 0
        Rectangle:
            pos: self.pos
            size: self.size
